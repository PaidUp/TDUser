{
  "name": "TDUser",
  "version": "1.2.0",
  "description": "microservices TDUser",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "clean:dist": "rm -rf dist",
    "clean:build": "rm -rf build",
    "create:dist": "mkdir dist",
    "create:build": "mkdir build",
    "test": "NODE_ENV=test mocha server/**/*.spec.js --reporter xunit > build/test-result.xml",
    "jenkins": "npm run clean:build && npm run create:build && npm test",
    "build:server": "cp -R server/ dist/server",
    "build:client": "cp -R client/ dist/public",
    "build": "npm run clean:dist && npm run create:dist && npm run build:server && npm run build:client"
  },
  "keywords": [
    "talos",
    "digital",
    "TDUser",
    "users"
  ],
  "author": "@talosdigital",
  "license": "ISC",
  "dependencies": {
    "TDCore": "https://github.com/talosdigital/TDCore/tarball/master",
    "blind": "1.0.0",
    "body-parser": "1.14.2",
    "composable-middleware": "0.3.0",
    "compression": "1.6.1",
    "connect-mongo": "1.1.0",
    "cookie-parser": "1.4.1",
    "ejs": "2.4.1",
    "email-templates": "2.1.0",
    "errorhandler": "1.4.3",
    "express": "4.13.4",
    "express-jwt": "3.3.0",
    "express-session": "1.13.0",
    "fb": "1.0.2",
    "is-valid-ssn": "0.0.2",
    "jsonwebtoken": "5.5.4",
    "lodash": "4.3.0",
    "method-override": "2.3.5",
    "moment": "2.11.2",
    "mongoose": "4.4.1",
    "morgan": "1.6.1",
    "node-uuid": "1.4.7",
    "nodemailer": "2.1.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pmx": "^0.6.1",
    "request": "^2.69.0",
    "serve-favicon": "2.3.0",
    "socket.io": "1.4.5",
    "socket.io-client": "1.4.5",
    "socketio-jwt": "4.3.4",
    "winston": "2.1.1",
    "winston-loggly": "^1.3.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "fake": "0.2.2",
    "faker": "3.0.1",
    "mockery": "1.4.0",
    "nock": "7.0.2",
    "open": "0.0.5",
    "requirejs": "2.1.22",
    "should": "8.2.1",
    "supertest": "1.1.0"
  },
  "engines": {
    "node": "4.1.0"
  },
  "private": true,
  "config": {
    "blanket": {
      "pattern": [
        "varity/lib"
      ]
    }
  }
}
